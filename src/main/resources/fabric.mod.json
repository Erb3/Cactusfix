  "authors": [
    "Erb3",
    "Contributors"
  ],

  "contact": {
    "website": "https://github.com/Erb3/Cactusfix",
    "repo": "https://github.com/Erb3/Cactusfix.git"
  },

  "license": "MPL-2.0",
  "icon": "assets/cactusfix/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "github.erb3.fabric.cactusfix.Main"
    ]
  },
  "mixins": [
    "cactusfix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric-api": "*",
